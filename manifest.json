{
  "name": "PasswordBag",
  "version": "1.0.3",
  "description": "Login to your accounts in  a click.",
  "icons": {
    "16": "img/logo16.png",
    "48": "img/logo48.png",
    "128": "img/logo128.png"
  },
  "browser_action": {
    "default_icon": "img/logo.png"
  },
  "background": {
    "persistent": false,
    "scripts": ["js/jquery-3.1.1.min.js", "js/background.js"]
  },
  "content_scripts": [{
    "matches":    ["*://*/*"],
    "js":         ["js/jquery-3.1.1.min.js", "js/jquery.noty.packaged.min.js", "js/content.js"],
    "run_at": "document_end",
    "all_frames": true
  }],
  "permissions": [
    "activeTab",
    "tabs",
    "*://*/*"
  ],
  "manifest_version": 2
}